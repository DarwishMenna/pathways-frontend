{
  "name": "pathways-frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@lingui/babel-preset-react": "2.1.0",
    "@lingui/cli": "2.1.0",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/expo": "^27.0.8",
    "@types/history": "^4.7.0",
    "@types/jest": "^22.2.2",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.20",
    "@types/react-redux": "^5.0.21",
    "@types/react-router-native": "^4.2.3",
    "@types/react-test-renderer": "^16.0.1",
    "@types/remote-redux-devtools": "^0.5.2",
    "@types/uuid": "^3.4.4",
    "babel-core": "^6.26.2",
    "babel-jest": "^22.4.3",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.3",
    "exp": "^55.0.5",
    "jest": "^22.4.3",
    "jest-expo": "28.0.0",
    "react-devtools": "^3.2.1",
    "react-dom": "^16.5.0",
    "react-native-scripts": "1.13.1",
    "react-test-renderer": "^16.3.1",
    "redux-devtools": "^3.4.1",
    "remote-redux-devtools": "^0.5.12",
    "ts-jest": "^22.4.2",
    "tslint": "^5.11.0",
    "tslint-immutable": "^4.7.0",
    "typescript": "^2.7.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "postinstall": "bin/postinstall.sh",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "build": "bin/buildFixtures.sh && tsc",
    "test": "yarn run build && yarn run jest --maxWorkers=4",
    "clean": "rimraf 'lib/**/*.js' 'lib/**/*.js.map' 'locale/_build/**/*.json'",
    "extract-strings": "yarn run build && lingui extract",
    "build-strings": "lingui compile --strict",
    "clean-strings": "rimraf 'locale/_build/**/*.json'",
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios"
  },
  "jest": {
    "preset": "jest-expo",
    "setupTestFrameworkScriptFile": "<rootDir>/lib/application/__tests__/helpers/setup_tests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|expo|@expo/.*|native-base)/)"
    ],
    "testRegex": "/__tests__/[-a-zA-Z0-9_]+\\.(test|spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@lingui/react": "2.1.0",
    "@types/query-string": "^6.1.0",
    "expo": "28.0.0",
    "history": "^4.7.2",
    "intl": "^1.2.5",
    "native-base": "^2.5.2",
    "query-string": "^6.1.0",
    "ramda": "^0.25.0",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-28.0.0.tar.gz",
    "react-native-collapsible": "^0.12.0",
    "react-native-dotenv": "^0.1.1",
    "react-native-markdown-renderer": "^3.2.8",
    "react-redux": "^5.0.7",
    "react-router-native": "^4.3.0",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0",
    "uuid": "^3.3.2"
  },
  "lingui": {
    "sourceLocale": "en",
    "format": "lingui",
    "srcPathDirs": [
      "<rootDir>/lib"
    ]
  }
}