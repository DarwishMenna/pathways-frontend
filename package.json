{
  "name": "pathways-frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@lingui/babel-preset-react": "2.0.1",
    "@lingui/cli": "2.0.1",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/history": "^4.6.2",
    "@types/jest": "^22.2.2",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.20",
    "@types/react-redux": "^5.0.15",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux-first-router": "^1.10.2",
    "@types/redux-first-router-link": "^1.4.3",
    "babel-core": "^6.26.2",
    "babel-jest": "^22.4.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "jest": "^22.4.3",
    "jest-expo": "26.0.0",
    "react-dom": "^16.3.1",
    "react-native-scripts": "1.13.1",
    "react-test-renderer": "^16.3.1",
    "redux-devtools": "^3.4.1",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "tslint-immutable": "^4.5.3",
    "typescript": "^2.7.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "postinstall": "bin/postinstall.sh",
    "lint": "tslint --rules-dir node_modules/tslint-immutable/rules/ src/**/*.ts src/**/*.tsx",
    "tsc": "tsc",
    "jest": "jest",
    "lingui": "lingui",
    "clean": "rimraf lib/**/*.js lib/**/*.js.map",
    "build": "yarn run clean && yarn run clean && yarn run clean && yarn run tsc",
    "test": "yarn run build && yarn run jest",
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios"
  },
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "<rootDir>/lib/application/test_helpers/setup_tests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/lib/**/*.test.js"
    ]
  },
  "dependencies": {
    "@lingui/react": "2.0.1",
    "@types/react-native": "^0.52.20",
    "expo": "^27.0.0",
    "react": "16.3.2",
    "react-native": "^0.55.0",
    "history": "^4.7.2",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-first-router": "^0.0.16-next",
    "redux-first-router-link": "^1.4.2",
    "redux-persist": "^5.9.1"
  },
  "lingui": {
    "sourceLocale": "en"
  }
}
